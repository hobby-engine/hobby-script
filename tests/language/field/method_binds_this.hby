struct Foo {
  var name;
  var func;

  fn say(a) {
    io:print(self.name);
    io:print(a);
  }
}

var foo1 = Foo {
  name = "foo1",
};

var foo2 = Foo {
  name = "foo2",
};

// Store the method reference on another object
foo2.func = foo1.say;
// Still retains original receiver
foo2.func(1);
// expect: foo1
// expect: 1
