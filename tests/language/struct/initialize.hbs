struct Foo {
  var x;
  var y = 2;
  var z;
}

var foo = Foo {
  x = 1,
};

var no_end_comma = Foo {
  x = 1,
  y = 3,
  z = 1
};

io:print(foo.x); // expect: 1
io:print(foo.y); // expect: 2
io:print(foo.z); // expect: null

io:print(no_end_comma.x); // expect: 1
io:print(no_end_comma.y); // expect: 3
io:print(no_end_comma.z); // expect: 1

